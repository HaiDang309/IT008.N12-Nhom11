<UserControl xmlns:FastFood="clr-namespace:FastFood"  x:Class="FastFood.Edit_discount_table.Normal_edit_control"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FastFood.Edit_discount_table"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label x:Name="Title_txt"
            VerticalAlignment="Top"
            HorizontalAlignment="Center"
            Content="Title goes here!!"/>
        <FastFood:Hint_textBox x:Name="Hint_editTxt"
            VerticalAlignment="Top"
            HorizontalAlignment="Center"
            Margin="0,30,0,0"
            Box_size="150"
            HintText="Custom hint goes here!!"/>
    </Grid>
</UserControl>
